{
  "name": "surfalive",
  "version": "1.0.0",
  "description": "Project from course Node-TypeScript-API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run-script build && node dist/test.js",
    "start:dev": "ts-node-dev --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SydFranco/surfalive.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "sydfranco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SydFranco/surfalive/issues"
  },
  "homepage": "https://github.com/SydFranco/surfalive#readme",
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "jest": "^26.0.1",
    "prettier": "^2.5.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "yarn": "^1.22.17"
  }
}
